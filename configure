	      && y (Apat, "\\\\", 0) && y (Apat, "A", 0)
	      && y (apat, "\\\\", 0) && y (apat, "a", 0)
	      && n (Apat, A_1, 0) == ('A' < '\\\\')
	      && n (apat, a_1, 0) == ('a' < '\\\\')
	      && y (Apat, A01, 0) == ('A' < '\\\\')
	      && y (apat, a01, 0) == ('a' < '\\\\')
	      && y (Apat, bs_1, 0) == ('A' < '\\\\')
	      && y (apat, bs_1, 0) == ('a' < '\\\\')
	      && n (Apat, bs01, 0) == ('A' < '\\\\')
	      && n (apat, bs01, 0) == ('a' < '\\\\')
