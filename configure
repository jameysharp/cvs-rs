{

  int numfds;
  fd_set readfds;
  struct timeval timeout;
  int fd = open ("/dev/null", O_RDONLY);

  FD_ZERO (&readfds);
  FD_SET (fd, &readfds);
  timeout.tv_sec = 0;
  timeout.tv_usec = 1;

  while ((numfds = select (fd + 1, &readfds, NULL, NULL, &timeout)) < 0
	 && errno == EINTR);
  return (numfds <= 0);

  ;
  return 0;
