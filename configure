	   static char const bs01[] = { '\\\\' + 1, 0 };
	   return
	    !(n ("a*", "", 0)
	      && y ("a*", "abc", 0)
	      && n ("d*/*1", "d/s/1", FNM_PATHNAME)
	      && y ("a\\\\bc", "abc", 0)
	      && n ("a\\\\bc", "abc", FNM_NOESCAPE)
	      && y ("*x", ".x", 0)
